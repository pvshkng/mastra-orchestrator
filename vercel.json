{
  "version": 2,
  "installCommand": "npm install --omit=dev",
  "buildCommand": "npm run build",
  "outputDirectory": ".mastra/output",
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.mjs"
    }
  ]
}
